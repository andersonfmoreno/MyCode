						iBitValue 	= iBit_L | iBit_R  ;
$$
						module_flat.MEAN_THROUGHPUT_REQUESTED =  iBitValue;

//TRACE2( ("\n    (decode_listofTrafficVol-16) STempOctet=(%s,%s) iBits=(%x,%x) iBitValue=%x", sTempOctet_L, sTempOctet_R, iBit_L, iBit_R, iBitValue) )

