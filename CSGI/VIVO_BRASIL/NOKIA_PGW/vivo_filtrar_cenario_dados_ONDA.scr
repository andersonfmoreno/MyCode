//TRACE2( ("\n    (decode_listofTrafficVol-15) STempOctet=(%s,%s) iBits=(%x,%x) iBitValue=%x", sTempOctet_L, sTempOctet_R, iBit_L, iBit_R, iBitValue) )

						module_flat.PEAK_THROUGHPUT_NEGOTIATED =  iBitValue;
						// -----------------------------------------------
						//     0123456789
    						//     11223344
						// Qos=0323331F9396102074FB1010
						// -----------------------------------------------
